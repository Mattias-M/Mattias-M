package hemläxa;

import java.util.Scanner;

public class v37 {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);

		System.out.println("vad är ditt namn?");
		String namn = scan.next();
	
		
		System.out.print("Hur gammal är du?");
		int ålder = scan.nextInt();
		
		System.out.println("Vad är din adress?");
		String adress = scan.next();
		
		System.out.println("Vad är ditt postnumer?");
		String postnummer = scan.next();
		
		System.out.println("Vilken stad kommer du i från?");
		String stad = scan.next();
		
		System.out.println("Telefonnummer?");
		String tel = scan.next();
		
		System.out.println("Tack" + namn + "Du är" + ålder + "och bor i" + adress + "postnummer är" + postnummer);
		
		
		
		
		
		
	}

}
